  <content><![CDATA[
bool is_prime(ll n){
	if(n<=1) return false;
	for(ll i=2;i*i<=n;i++){
		if(n%i==0)  return false;
	}
	return true;
}
]]></content>
  <tabTrigger>ckprime</tabTrigger>
  <description>Check prime or not</description>
