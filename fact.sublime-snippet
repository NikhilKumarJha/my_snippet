  <content><![CDATA[
vector<ll> fact(1000005);
void factorial(){
	fact[0]=fact[1]=1;
	for(ll i=2;i<fact.size();i++){
		fact[i]=fact[i-1]*i;
	}
}
]]></content>
  <tabTrigger>fact</tabTrigger>
  <description>Factorial</description>
