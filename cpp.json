			"ll fpm(ll x,ll y,ll mod){",
			"	ll res=1;",
			"	while(y>0){",
			"		if(y&1LL){",
			"			res=(res*x)%mod;",
			"		}",
			"		y=y>>1LL;",
			"		x=(x*x)%mod;",
			"	}",
			"	return (res%mod);",
			"}",
